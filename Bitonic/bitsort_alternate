		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
		for(int i=56;i<60;i++)
		{
		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
