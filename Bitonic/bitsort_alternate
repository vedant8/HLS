void bitonic4()
{
	int temp;
#pragma HLS dataflow
// comparators in blocks of 4
	for(int i=0;i<2;i++)
	{
		#pragma HLS unroll
			if(a[i]>a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=4;i<6;i++)
	{
#pragma HLS unroll
		if(a[i]<a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=8;i<10;i++)
	{
#pragma HLS unroll
			if(a[i]>a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=12;i<14;i++)
	{
#pragma HLS unroll
			if(a[i]<a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=16;i<18;i++)
	{
#pragma HLS unroll
			if(a[i]>a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=20;i<22;i++)
	{
#pragma HLS unroll
			if(a[i]<a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=24;i<26;i++)
	{
#pragma HLS unroll
			if(a[i]>a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
							}
	}
	for(int i=28;i<30;i++)
	{
#pragma HLS unroll
			if(a[i]<a[i+2])
							{temp=a[i];
							a[i]=a[i+2];
							a[i+2]=temp;
