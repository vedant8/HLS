		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
			for(int i=96;i<100;i++)
		{
		#pragma HLS unroll
		if(a[i]>a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
		 for(int i=104;i<108;i++)
		{
		#pragma HLS unroll
		if(a[i]>a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
		 for(int i=112;i<116;i++)
		{
		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
		for(int i=120;i<124;i++)
		{
		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
			}
	for(int i=0;i<13;i=i+4)
	{
	#pragma HLS unroll
	if(a[i]>a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}

		if(a[i+1]>a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
	for(int i=16;i<29;i=i+4)
	{
	#pragma HLS unroll
	if(a[i]<a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}

		if(a[i+1]<a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
	for(int i=32;i<45;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]>a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}

		if(a[i+1]>a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
	for(int i=48;i<63;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]<a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}
		if(a[i+1]<a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
		for(int i=64;i<77;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]>a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}

		if(a[i+1]>a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
	for(int i=80;i<93;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]<a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}
		if(a[i+1]<a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
		for(int i=96;i<109;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]>a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}

		if(a[i+1]>a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}
	for(int i=112;i<125;i=i+4)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]<a[i+2])
		{
			temp=a[i+2];
			a[i+2]=a[i];
			a[i]=temp;
		}
		if(a[i+1]<a[i+3])
		{
			temp=a[i+3];
			a[i+3]=a[i+1];
			a[i+1]=temp;
		}
		}

	for(int i=0;i<15;i=i+2)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]>a[i+1])
		{
			temp=a[i+1];
			a[i+1]=a[i];
			a[i]=temp;
		}

		}
	for(int i=16;i<31;i=i+2)
	{
	#pragma HLS unroll//may be faster if split into two loops
	if(a[i]<a[i+1])
		{
			temp=a[i+1];
			a[i+1]=a[i];
			a[i]=temp;
		}

		}
	for(int i=32;i<47;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]>a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}
		for(int i=48;i<63;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]<a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}
			for(int i=64;i<79;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]>a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}
		for(int i=80;i<95;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]<a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}
			for(int i=96;i<111;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]>a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}
		for(int i=112;i<127;i=i+2)
		{
		#pragma HLS unroll//may be faster if split into two loops
		if(a[i]<a[i+1])
			{
				temp=a[i+1];
				a[i+1]=a[i];
				a[i]=temp;
			}

			}

bitonic32();
	}

void bitonic8()
{// sorting blocks of size 8
	int temp;
#pragma HLS dataflow
	for(int i=0;i<4;i++)
	{
		#pragma HLS unroll
		if(a[i]>a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
	}
	for(int i=8;i<12;i++)
	{
		#pragma HLS unroll
		if(a[i]<a[i+4])
			{
				temp=a[i+4];
				a[i+4]=a[i];
				a[i]=temp;
			}
	}
	for(int i=16;i<20;i++)
	{
